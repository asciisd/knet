{
    "name": "asciisd/knet",
    "description": "Knet package is provides an expressive, fluent interface to KNet's payment services.",
    "keywords": [
        "Laravel",
        "billing",
        "Knet",
        "Knet Payment"
    ],
    "license": "MIT",
    "support": {
        "issues": "https://github.com/asciisd/knet/issues",
        "source": "https://github.com/asciisd/knet"
    },
    "authors": [
        {
            "name": "Amr Ahmed",
            "email": "aemad@asciisd.com"
        }
    ],
    "require": {
        "php": "^8.3",
        "ext-libxml": "*",
        "ext-simplexml": "*",
        "illuminate/contracts": "^10.0|^11.0|^12.0",
        "illuminate/database": "^10.0|^11.0|^12.0",
        "illuminate/http": "^10.0|^11.0|^12.0",
        "illuminate/log": "^10.0|^11.0|^12.0",
        "illuminate/routing": "^10.0|^11.0|^12.0",
        "illuminate/support": "^10.0|^11.0|^12.0",
        "illuminate/events": "^10.0|^11.0|^12.0"
    },
    "require-dev": {
        "phpunit/phpunit": "^10.0|^11.0",
        "orchestra/testbench": "^8.0|^9.0",
        "mockery/mockery": "^1.6",
        "phpstan/phpstan": "^1.10",
        "laravel/pint": "^1.0"
    },
    "autoload": {
        "psr-4": {
            "Asciisd\\Knet\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Asciisd\\Knet\\Tests\\": "tests/"
        }
    },
    "scripts": {
        "test": "vendor/bin/phpunit",
        "test-coverage": "vendor/bin/phpunit --coverage-html coverage",
        "format": "vendor/bin/pint",
        "analyse": "vendor/bin/phpstan analyse"
    },
    "extra": {
        "laravel": {
            "providers": [
                "Asciisd\\Knet\\Providers\\KnetServiceProvider"
            ]
        }
    },
    "config": {
        "sort-packages": true
    },
    "minimum-stability": "stable",
    "prefer-stable": true
}
