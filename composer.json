{
    "name": "asciisd/knet",
    "description": "Knet package is provides an expressive, fluent interface to Knet's payment services.",
    "keywords": ["Laravel", "billing", "Knet", "Knet Payment"],
    "license": "MIT",
    "support": {
        "issues": "https://github.com/asciisd/knet/issues",
        "source": "https://github.com/asciisd/knet"
    },
    "authors": [
        {
            "name": "Amr Ahmed",
            "email": "aemad@asciisd.com"
        }
    ],
    "require": {
        "php": "^7.3",
        "ext-json": "*",
        "ext-intl": "*",
        "dompdf/dompdf": "^0.8.0",
        "illuminate/contracts": "^7.0|^8.0",
        "illuminate/database": "^7.0|^8.0",
        "illuminate/http": "^7.0|^8.0",
        "illuminate/log": "^7.0|^8.0",
        "illuminate/notifications": "^7.0|^8.0",
        "illuminate/routing": "^7.0|^8.0",
        "illuminate/support": "^7.0|^8.0",
        "illuminate/view": "^7.0|^8.0",
        "moneyphp/money": "^3.2",
        "nesbot/carbon": "^2.0",
        "symfony/http-kernel": "^5.0",
        "symfony/intl": "^5.0"
    },
    "require-dev": {
        "mockery/mockery": "^1.0",
        "orchestra/testbench": "^5.0|^6.0",
        "phpunit/phpunit": "^8.0|^9.0",
        "phpstan/phpstan": "^0.12.9"
    },
    "suggest": {
        "ext-intl": "Allows for more locales besides the default \"en\" when formatting money values."
    },
    "autoload": {
        "psr-4": {
            "Asciisd\\Knet\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Asciisd\\Knet\\Tests\\": "tests/"
        }
    },
    "extra": {
        "branch-alias": {
            "dev-master": "1.x-dev"
        },
        "laravel": {
            "providers": [
                "Asciisd\\Knet\\Providers\\KnetServiceProvider"
            ]
        }
    },
    "config": {
        "sort-packages": true
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
