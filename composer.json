{
    "name": "asciisd/knet",
    "description": "Knet package is provides an expressive, fluent interface to Knet's payment services.",
    "keywords": [
        "Laravel",
        "billing",
        "Knet",
        "Knet Payment"
    ],
    "license": "MIT",
    "support": {
        "issues": "https://github.com/asciisd/knet/issues",
        "source": "https://github.com/asciisd/knet"
    },
    "authors": [
        {
            "name": "Amr Ahmed",
            "email": "aemad@asciisd.com"
        }
    ],
    "require": {
        "php": "^7.1.3",
        "ext-json": "*",
        "dompdf/dompdf": "^0.8.0",
        "illuminate/contracts": "~5.8.0|^6.0|^7.0",
        "illuminate/database": "~5.8.0|^6.0|^7.0",
        "illuminate/http": "~5.8.0|^6.0|^7.0",
        "illuminate/log": "~5.8.0|^6.0|^7.0",
        "illuminate/notifications": "~5.8.0|^6.0|^7.0",
        "illuminate/routing": "~5.8.0|^6.0|^7.0",
        "illuminate/support": "~5.8.0|^6.0|^7.0",
        "illuminate/view": "~5.8.0|^6.0|^7.0",
        "moneyphp/money": "^3.2",
        "nesbot/carbon": "^2.0",
        "symfony/http-kernel": "^4.3|^5.0",
        "symfony/intl": "^4.3|^5.0",
        "ext-openssl": "*"
    },
    "require-dev": {
        "mockery/mockery": "^1.0",
        "orchestra/testbench": "^3.8|^4.0|^5.0",
        "phpunit/phpunit": "^7.5|^8.0"
    },
    "suggest": {
        "ext-intl": "Allows for more locales besides the default \"en\" when formatting money values."
    },
    "autoload": {
        "psr-4": {
            "Asciisd\\Knet\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Asciisd\\Knet\\Tests\\": "tests/"
        }
    },
    "extra": {
        "branch-alias": {
            "dev-master": "1.x-dev"
        },
        "laravel": {
            "providers": [
                "Asciisd\\Knet\\Providers\\KnetServiceProvider"
            ]
        }
    },
    "config": {
        "sort-packages": true
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
